{{#partial "content"}}

        <h2> CURRENT HEROES</h2>
        <div class="heroes">
            {{#each hero}}
                <div class="hero-item">
                    <p>Name: {{ name }} </p>
                    <p>Age: {{ age }}</p>
                    <p>Special power: {{ speciaPower }}</p>
                    <p>Weakness: {{ weakness }}</p>
                    <p>Id: {{ id }}</p>

                </div>

            {{/each}}
        </div>
{{/partial}}
{{> layout.hbs}}